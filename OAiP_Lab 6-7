#include <iostream>
using namespace std;

void combsort(int n, int array[])
{
    float k = 1.247;
    int step = n - 1;
    while(step >= 1)
    {
        int i = 0;
        while(i < n - step)
        {
            if(array[i] > array[i + step])
            {
                swap(i, i + step)
            }
        i++;
        }
    step /= k;
    }
}

int main()
{
    int array[], n;
    cout << "" << endl;
    cin >> n;
    cout << "" << endl;
    
    for(i = 0; i >= n; i++)
    {
        cin >> array[i];
    }
    
    switch(selector)
    {
        case 1:
            combsort(n, array[])
        case 2:
            forcombsort(n, array[])
        case 3:
            return 0;
    }
}
