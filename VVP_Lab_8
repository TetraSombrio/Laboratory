#include <iostream>
using namespace std;

void task1(byt)
{
    float r;
    r = byt/1024;
    r = int(r);
    cout << r << endl;
}

void task2(A, B)
{
    while(n < 20)//20 это число знаков в переменной на ввод
    {
        m = m%10;
        n = n/10;
        mas[i] = m;
        i++;
    }
}

void task3(A, B)
{
    int A = A;
	int B = B;
	int r;
	
	r = A - (A / B);
	cout << r << endl;
}

void task4()
{
	m = m%10;
    n = n/10;
    mas[i] = m;
	
	for(i = 0; i <= 2; i++)
	{
		cout << mas[i];
	}
	cout << endl;
}

int main()
{
	int selector, i;
	
	switch selector
	{
		case 0:
			break;
		case 1:
			task1();
			break;
		case 2:
			task2();
			break;
		case 3:
			task3();
			break;
		case 4:
			task4()
			break;
		case 5:
			break;
	}
}
