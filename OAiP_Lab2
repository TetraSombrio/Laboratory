#include <iostream>
#include <cmath>
using namespace std;

int a,b,c;

int thefirst(int a, int b, int c)
{
	int di;
	di = (b^2)-4*a*c;
	return di;
}

int thesecond(int a, int b, int c)
{
    int r;
    r = a*8-b+2*c;
    if(r>=0){
        cout << r << endl;
        return r;
    }
    else {
        cout << "err";
        return 1;
    }
}

int thethird(int a, int b)
{
    int r;
    r = pow(a,2) - 16 + b * 2;
    while (r != 2 * b)
    {
        a++;
        b++;
        return r;
    }
}

int main()
{
    cout << "введите переменные" << endl;
    cin >> a >> b >> c;
    cout << thefirst(a,b,c) << endl;
    cout << thesecond(a,b,c) << endl;
    cout << thethird(a,b) << endl;
    return 0;
}
