}

void f6()
{
    double bucks, yens, marks, funts, franks;
    
    cout << "write" << endl;
    cin >> bucks;
    
    yens = bucks * 0.00955;
    marks = bucks * 0.584;
    funts = bucks * 1.487;
    franks = bucks * 0.172;
    
    cout << funts << endl << yens << endl << marks << franks << endl;
}

void f7()
{
    int celsys, farenheit;
    
    cout << "write" << endl;
    cin >> celsys;
    
    farenheit = celsys * ((9/5) + 32);
    cout << farenheit << endl;
}

void f8()
{
    
    cout << 1990 << setw(8) << setfill(".") << 135 << endl;
    << 1991 << setw(12) << setfill(",") << 7290 << endl;
    << 1992 << setw(6) << setfill("/") << 11300 << endl;
    << 1993 << setw(10) << setfill("'") << 16200 << endl;
}

void f9();
{
    double r, a, b, c, d;
    
    cout << "consistently write a, b, c, d" << endl;
    cin >> a >> b >> c >> d;
    r = (a*d + b*c)/(b*d);
    cout << "result a/b + c/d is " << r << endl;
}

